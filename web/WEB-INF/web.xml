<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>AHPRegistration</servlet-name>
        <servlet-class>controllers.AHPRegistration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BLC_ULB_DPR_Update</servlet-name>
        <servlet-class>controllers.BLC_ULB_DPR_Update</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sendmail1</servlet-name>
        <servlet-class>sendmail1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendMail</servlet-name>
        <servlet-class>controllers.SendMail</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AHPRegistration</servlet-name>
        <url-pattern>/AHPRegistration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BLC_ULB_DPR_Update</servlet-name>
        <url-pattern>/BLC_ULB_DPR_Update</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sendmail1</servlet-name>
        <url-pattern>/sendmail1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendMail</servlet-name>
        <url-pattern>/SendMail</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
